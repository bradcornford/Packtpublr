    "keywords": ["packtpub", "learning", "free", "ebook", "redeem", "collect"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bradley Cornford",
            "email": "me@bradleycornford.co.uk"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "4.*",
        "symfony/console": "3.*"
    },
    "require-dev": {
        "phpspec/phpspec": "2.*",
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.*"
    },
    "autoload": {
        "psr-0": {
            "Cornford\\Packtpublr": "src/"
        }
    },
    "minimum-stability": "stable"
}
